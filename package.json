{
  "name": "thetavern",
  "version": "1.0.0",
  "description": "Fantasy-Tabletop-RPG-themed multiplayer game consisting of minigames.",
  "main": "src/app.ts",
  "scripts": {
    "start": "node src/app.ts",
    "server": "ts-node-dev --respawn --transpile-only src/app.ts",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\""
  },
  "author": "Megan McNulty",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "mongoose-unique-validator": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/validator": "^13.1.3",
    "nodemon": "^2.0.4",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
